<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- 圆角 -->
    <corners
        android:radius="33dp"
        android:topLeftRadius="33dp"
        android:topRightRadius="33dp"
        android:bottomLeftRadius="33dp"
        android:bottomRightRadius="33dp"/><!-- 设置圆角半径 -->


    <!-- 间隔 -->
    <padding
        android:left="2dp"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp"/><!-- 各方向的间隔 -->

    <!-- 大小 -->
    <size
        android:width="50dp"
        android:height="66dp"/><!-- 宽度和高度 -->

    <!-- 填充 -->
    <solid
        android:color="#33000000"/><!-- 填充的颜色 -->

    <!-- 描边 -->
    <stroke
        android:width="2px"
        android:color="#fff67b29"
        />

</shape>