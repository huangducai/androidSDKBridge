    <style name="fly_sdk_button">
        <item name="android:textSize">@dimen/game_update_btn_text_size</item>
        <item name="android:textColor">#fff67b29</item>
        <item name="android:background">@drawable/fly_round_btn_bg</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">@dimen/game_update_btn_height</item>
    </style>
    <style name="fly_sdk_button_container">
        <item name="android:paddingLeft">@dimen/game_update_btn_container_paddingX</item>
        <item name="android:paddingRight">@dimen/game_update_btn_container_paddingX</item>
        <item name="android:paddingBottom">@dimen/game_update_btn_container_padding_bottom</item>
    </style>
    <style name="fly_sdk_content_downloading_text_view">
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">24dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:textColor">#ff999999</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">12dp</item>
    </style>
    <style name="fly_sdk_content_text_view">
        <item name="android:gravity">center_vertical</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:layout_marginTop">@dimen/game_update_msg_top_margin</item>
        <item name="android:layout_marginBottom">@dimen/game_update_msg_bottom_margin</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">14dp</item>
        <item name="android:minLines">2</item>

    </style>
    <style name="fly_sdk_content_view">
        <item name="android:background">#b2141523</item>
    </style>
    <style name="fly_sdk_dialog">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="fly_sdk_dialog_theme" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/fly_dialog_bg</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="fly_sdk_title_text">
        <item name="android:textSize">@dimen/game_update_title_text_size</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textColor">#fff67b29</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:layout_height">20.0dp</item>
    </style>
